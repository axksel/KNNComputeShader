#pragma kernel CSMain

int amountOfObjects;
float3 playerPos;


RWStructuredBuffer<float3> RP;
RWStructuredBuffer<float> Distance;

[numthreads(8,8,1)]
void CSMain (uint3 id : SV_DispatchThreadID)
{

	int index =id.x*id.y;

	if (index < amountOfObjects) {
		Distance[index] =distance(RP[index],playerPos);
       
       
	}
    

}
